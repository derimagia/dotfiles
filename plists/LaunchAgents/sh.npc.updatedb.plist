<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>sh.npc.updatedb</string>
	<key>StartCalendarInterval</key>
	<array>
		<dict>
			<key>Hour</key>
			<integer>23</integer>
			<key>Minute</key>
			<integer>30</integer>
		</dict>
	</array>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/bin/updatedb</string>
		<string>--localpaths=$HOME /usr</string>
	</array>
	<key>EnvironmentVariables</key>
	<dict>
		<key>LC_ALL</key>
		<string>C</string>
	</dict>
	<key>ProcessType</key>
	<string>Background</string>
	<key>StandardOutPath</key>
	<string>$HOME/Library/Logs/updatedb/plist-updatedb.log</string>
	<key>StandardErrorPath</key>
	<string>$HOME/Library/Logs/updatedb/plist-updatedb.err.log</string>
</dict>
</plist>
